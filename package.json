    "build": "tsup",
    "coverage": "vitest run --coverage",
    "release": "release-it --ci"
  },
  "license": "MIT",
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
