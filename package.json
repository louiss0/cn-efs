    "dotenv-cli": "^7.4.2",
    "release-it": "^17.6.0",
    "release-it-pnpm": "^4.6.3",
    "tsup": "^8.2.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "tailwindcss",
    "unocss",
    "css",
    "className",
    "classList",
    "windicss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/louiss0/cn-efs.git"
  },
  "author": {
    "name": "Shelton Louis",
