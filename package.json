{
  "name": "@code-fixer-23/cn-efs",
  "version": "3.0.0-beta.1",
  "description": "This is the repo for creating a Class Name Filter Sorter",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
